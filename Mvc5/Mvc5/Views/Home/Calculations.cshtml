@model IEnumerable<Mvc5.Models.Calculation>

@{
    ViewBag.Title = "Calculations";
}

<h2>Calculations</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Num1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Num2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Operation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Result)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Num1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Num2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Operation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Result)
        </td>
       
    </tr>
}

</table>
